@model List<MoviesModel>

@{
    ViewData["Title"] = "Movie Results!";
}


<h1>@ViewData["Title"]</h1>
<hr />

@foreach (var item in Model)
{
    <div class="row">
        <div class="col-sm">
            @item.Title,
            @item.Genre
        </div>
    </div>
}